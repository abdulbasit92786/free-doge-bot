let payload = JSON.parse(content.text);

if (payload.command === "withdraw_from_webapp") {
  let wallet = payload.wallet;
  let amount = payload.amount;

  let log = `ðŸ“¥ *New Withdraw Request*\n\nðŸ‘¤ User: [${user.first_name}](tg://user?id=${user.telegramid})\nðŸ’¼ Wallet: \`${wallet}\`\nðŸ’° Amount: *${amount} DOGE*`;

  Bot.runCommand("withdraw_from_webapp", {
    wallet: wallet,
    amount: amount
  });
}
